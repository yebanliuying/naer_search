body {margin:0;padding:0;}
img {border:0;}

.wrapper{
    min-height: 100%;
    position: relative;
    padding-bottom: 35px;
}

.search-box{
    position: relative;
    margin:170px auto 0;
    width: 640px;
    padding-right: 10px;
}
.search-box .glogo{
    background: url("../img/logo.png") no-repeat;
    height: 88px;
    width: 374px;
    margin: 0 auto;
}
.search-box .s_box {
    padding: 50px 35px;
    height: 50px;
    width: 640px;
}
.s_text{
    float: left;
    font-size: 14px;
    width: 520px;
    background-color: #FFFFFF;
    border-color: #C0C0C0 #D9D9D9 #D9D9D9;
    border-style: solid;
    border-width: 1px;
    color: #333333;
    display: inline-block;
    height: 26px;
    line-height: 16px;
    line-height: 28px\9;
    *+line-height: 28px;
    padding-left: 8px;
    vertical-align: bottom;
    -moz-border-bottom-colors: none;
    -moz-border-image: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
}
.s_text:hover {
    -moz-border-bottom-colors: none;
    -moz-border-image: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-color: #A0A0A0 #B9B9B9 #B9B9B9;
    border-style: solid;
    border-width: 1px;
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1) inset;
}
.s_text:focus {
    /*border: 1px solid #4D90FE;
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.3) inset;*/
    outline: medium none;
}

.search_bottom{
    margin-left: 124px;
    width: 650px;
    overflow: hidden;
}

.m-button {
    display: inline-block;
    width: 85px;
    text-align: center;
    vertical-align: middle;
    font-size: 13px;
    font-weight: bold;
    height: 30px;
    padding: 0 8px;
    margin: 0;
    line-height: 30px;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    -webkit-transition: all 0.218s;
    -moz-transition: all 0.218s;
    -ms-transition: all 0.218s;
    -o-transition: all 0.218s;
    transition: all 0.218s;
    border: 1px solid rgba(0,0,0,0.1);
    -webkit-user-select: none;
    -moz-user-select: none;
    user-select: none;
    cursor: default;
    margin-right: 10px;
    outline: 0;
    border: 1px solid #38f;
    *border:0;
    box-shadow: 0 1px 1px rgba(0,0,0,0.2);
    -webkit-box-shadow: 0 1px 1px rgba(0,0,0,0.2);
    -moz-box-shadow: 0 1px 1px rgba(0,0,0,0.2);
    background-color: #38f;
    color: #fff;
}
.m-button:hover {
    text-decoration: none;
    background: #317ef3;
}
.m-button:active {
    background-image: -webkit-linear-gradient(top,#f6f6f6,#f1f1f1);
    background-image: -moz-linear-gradient(top,#f6f6f6,#f1f1f1);
    background-image: -ms-linear-gradient(top,#f6f6f6,#f1f1f1);
    background-image: -o-linear-gradient(top,#f6f6f6,#f1f1f1);
    background-image: linear-gradient(top,#f6f6f6,#f1f1f1);
    -webkit-box-shadow: inset 0 1px 2px rgba(0,0,0,0.3);
    -moz-box-shadow: inset 0 1px 2px rgba(0,0,0,0.3);
    box-shadow: inset 0 1px 2px rgba(0,0,0,0.3);
}

.s_submit{
    margin-left: 10px;
    width: 80px;
    background: #317ef3;
}

.search-result{
}
.header-bar{
    position: relative;
    background: none repeat scroll 0 0 #f1f1f1;;
    background-image: -webkit-gradient(radial,100 36,0,100 -40,120,from(#fafafa),to(#f1f1f1));
    border-bottom: 1px solid #e5e5e5;
    height: 56px;
    padding-top:17px;
    /*padding-bottom:17px;*/
}
.logo{
    display:inline-block;
    padding-left:16px;
    padding-right:12px;
    float: left;
}
.logo a{
    display: block;
    height: 37px;
    margin: 0;
    overflow: hidden;
    position: relative;
    width: 95px;
}
.logo img{
    left: 0;
    position: absolute;
    top: 2px;
}
.form_box{
    display:inline-block;
    margin-bottom: 40px;
}
.form_box .s_text{
    padding: 4px 6px;
    border-spacing: 2px;
    color: #222;
    height:20px;
    width:529px;
    font: 17px;
}

.form_box .s_text:hover{
    box-shadow: none;
    -webkit-box-shadow: none;
    -moz-box-shadow: none;
    border:1px solid gray;
}
.form_box .s_text:active,
.form_box .s_text:focus{
    -webkit-box-shadow: none;
    -moz-box-shadow: none;
    box-shadow: none;
    border: 1px solid #4D90FE;
}
.form_box .s_submit{
    margin-left: 16px;
    width:72px;
    background:#38f;
}
.form_box .s_submit:hover{
    background: #317ef3;
}
.content .content_header{
    height:40px;
    border-bottom:1px solid #ebebeb;
    line-height: 35px;
}
.link{
    padding-left:120px;
}
.notice_top{
    float:right;
    font-size:12px;
    color:#999;
}
.content_header a{
    display: inline-block;
    font-size: small;
    height: 35px;
    margin: 2px 3px 0;
    padding: 0 8px;
    text-decoration:none;
    color:#777;
}
.content_header a:hover{
    color:#555;
}
.content_header a.active,
.content_header a.active:hover{
    border-bottom:3px solid #dd4b39;
    font-weight:bold;
    color:#dd4b39;
}
.result_stats{
    color:#808080;
    margin-left:123px;
    font-size:small;
    line-height: 35px;
    margin-bottom:10px;
    margin-top: 5px;
}

.search_result{
    margin-left:124px;
    width:650px;
    overflow: hidden;
}
.notice{
    float:right;
    background-color: #ffffd9;
    border:1px solid #f3f3f3;
    color:#333;
    width:400px;
    height:150px;
    padding:10px;
    font-size:13px;
    margin-right:10px;
    line-height:20px;
}
.hidden{
    display:none;
}
.notice h1{
}
.notice a{
    color:#1a0dab;

}
.search_result .s_r{
    margin-bottom:23px;
}
.s_r .title{
    color: #1A0DAB;
    font-size: 18px;
    text-decoration: none;
}
.s_r .title:hover{
    text-decoration: underline;
}
.s_r .title em{
    color:#dd4b39;
    font-style:normal;
}
.s_r .visible_url{
    font-size: 13px;
    color:#006621;
    margin-bottom: 2px;
}
.s_r .min_content{
    line-height: 1.4;
    word-wrap: break-word;
    color: #545454;
    font-size:13px;
}
.s_r .min_content .gl{
    text-decoration:none;
    color:#1a0dab;
}
.s_r .min_content em,.s_r .min_content b{
    color: #dd4b39;
    font-style: normal;
    font-weight: normal;
}

.navcnt{
    border-top:1px solid #ededed;
    margin-left: 138px;
    width: 600px;
    margin-bottom: 50px;
}
.tr{

}
.td{
    text-align: center;
}
a.cur{
    font-weight: bold;
    color:#222;
}
a.cur:hover{
    text-decoration:none;
}
.csb{
    height: 40px;
    display: block;
    overflow: hidden;
    white-space: nowrap;
    color: #1a0dab;
}
.fl{
    text-decoration: none;
    /*display: block;*/
    color: #1a0dab;
    font-size: small;
    text-align: center;
}
.fl:hover{
    text-decoration: underline;
}
.pn{
    color:#1a0dab;      /* #609 */
    font-size:small;
    font-weight:bold;
}
.pn .txt:hover{
    text-decoration:underline;
}

.page_nav{margin: 40px auto 40px 0px;}
.page_nav li{list-style:none;float:left;padding:3px;}
.page_nav a strong{color:white;}
.page_nav a{display:block;padding:6px 12px 5px 12px;text-decoration:none;color:#666;font-weight:bold;}
.page_nav a:link,.page_nav a:visited{background:#DDDDDD; 
                                    -webkit-transition:all .2s ease;
                                       -moz-transition:all .2s ease;
                                        -ms-transition:all .2s ease;
                                         -o-transition:all .2s ease;
                                            transition:all .2s ease;
}
.page_nav a:hover,.page_nav a:active{background:#4D90FE;color:white;}
.page_nav a.curr{background:#4D90FE;color:white !important;}

.baidu-box, .bdSug_app, .bdsug_copy{display: none;}
.bdSug_wpr table tr td{padding:6px;}
.related_kwy_words{width:650px;margin-left:124px;list-style:none;padding:0;}
.related_kwy_words li{margin:0;width:45%;float:left;overflow:hidden;font-size:14px;padding:4px;padding-left:0;}

.top_nav{list-style:none;background:#222;margin:0;padding:0;height:30px;padding-left:20px;}
.top_nav li{float:left;line-height:30px;position:relative;border-top:1px solid #222;}
.top_nav li a{display:block;color:#ccc;text-decoration:none;font:13px/27px Arial,sans-serif;padding:0 8px;}
.top_nav li a:hover{background-color:#444;}
.top_nav li.current{border-top:1px solid red;}
.top_nav li.current a{font-weight:bold;color:white;}
.image_wrapper{width:100%;height:100%;}
